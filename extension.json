{
	"name": "ChessBrowser",
	"version": "0.4.0",
	"author": [
		"Wugapodes",
		"Kipod",
		"DannyS712"
	],
	"descriptionmsg": "chessbrowser-desc",
	"type": "parserhook",
	"license-name": "GPL-3.0-or-later",
	"url": "https://www.mediawiki.org/wiki/Extension:ChessBrowser",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"AutoloadClasses": {
		"ChessBrowser": "includes/ChessBrowser.php",
		"ChessBrowserHooks": "includes/ChessBrowserHooks.php",
		"ChessBrowserException": "includes/ChessBrowserException.php",
		"ChessParser": "includes/ChessParser.php",
		"GameParser": "includes/PgnParser/GameParser.php",
		"PgnGameParser": "includes/PgnParser/PgnGameParser.php",
		"MoveBuilder": "includes/PgnParser/MoveBuilder.php",
		"ChessJson": "includes/PgnParser/ChessJson.php",
		"FenParser0x88": "includes/PgnParser/FenParser0x88.php",
		"ChessSquare": "includes/ChessSquare.php",
		"ChessPiece": "includes/ChessPiece.php",
		"NotationAnalyzer": "includes/NotationAnalyzer.php",
		"CastlingTracker": "includes/CastlingTracker.php",
		"SquareRelations": "includes/SquareRelations.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "ChessBrowserHooks::onParserFirstCallInit",
		"OutputPageParserOutput": "ChessBrowserHooks::onOutputPageParserOutput"
	},
	"ResourceModules": {
		"ext.chessViewer": {
			"scripts": [
				"modules/ext.chessViewer.core.js"
			],
			"styles": [
				"modules/ext.chessViewer.css"
			],
			"dependencies": [
				"jquery.ui"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "ChessBrowser"
	},
	"MessagesDirs": {
		"ChessBrowser": [
			"i18n"
		]
	},
	"TrackingCategories": [
		"chessbrowser-invalid-category"
	],
	"manifest_version": 2
}
